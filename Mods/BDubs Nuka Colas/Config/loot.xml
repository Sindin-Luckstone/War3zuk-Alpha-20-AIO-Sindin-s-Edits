<configs>

<insertAfter xpath="/lootcontainers/lootcontainer[@id='124']">

	<lootcontainer id="154" count="2" size="4,4" sound_open="UseActions/open_chest" sound_close="UseActions/close_chest" open_time="2" loot_quality_template="qualPlus1Template">
		<item group="NukaGroup" prob="1"/>
	</lootcontainer>
	
	<lootcontainer id="155" count="1" size="3,3" sound_open="UseActions/open_safe" sound_close="UseActions/close_safe" open_time="2" loot_quality_template="qualPlus1Template">
		<item name="bookNukaRecipes" count="1" prob="1"/>
	</lootcontainer>
	
</insertAfter>

<insertBefore xpath="/lootcontainers/lootgroup[@name='groupQuestChallenge']">

	<lootgroup name="NukaGroup" count="all">
		<item name="drinkNukaCola" count="1,8"/>
		<item name="drinkNukaCherry" count="1,3"/>
		<item name="drinkNukaQuartz" count="1,2"/>
		<item name="drinkNukaQuantum" count="1"/>
	</lootgroup>
	
	<lootgroup name="NukaVendGroup">
		<item name="drinkNukaCola"/>
		<item name="drinkNukaCherry"/>
		<item name="drinkNukaQuartz"/>
		<item name="drinkNukaQuantum"/>
	</lootgroup>
	
	<lootgroup name="NukaCola" count="all">
		<item name="drinkNukaCola" count="1"/>
	</lootgroup>
	
  	<lootgroup name="NukaCherry" count="all">
		<item name="drinkNukaCherry" count="1"/>
	</lootgroup>

  	<lootgroup name="NukaQuantum" count="all">
		<item name="drinkNukaQuartz" count="1"/>
	</lootgroup>

  	<lootgroup name="NukaQuartz" count="all">
		<item name="drinkNukaQuantum" count="1"/>
	</lootgroup>

</insertBefore>

