		<spawn maxcount="1" respawndelay="2" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
		<spawn maxcount="1" respawndelay="2" time="Any" entitygroup="EnemyAnimalsDesert" spawnDeadChance="0" />
</append>

<append xpath="/spawning/biome[@name='wasteland']">
