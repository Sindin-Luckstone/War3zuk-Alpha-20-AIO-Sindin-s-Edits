<configs>
	
<append xpath="/spawning/biome[@name='pine_forest']">
		<spawn maxcount="3" respawndelay="1" time="Day" entitygroup="ZombiesAll" />
		<spawn maxcount="4" respawndelay="1" time="Night" entitygroup="ZombiesNight" />
		<spawn maxcount="1" respawndelay="2" time="Any" entitygroup="WildGameForest" spawnDeadChance="0" />
		<spawn maxcount="1" respawndelay="2" time="Night" entitygroup="EnemyAnimalsForest" spawnDeadChance="0" />
</append>

<append xpath="/spawning/biome[@name='burnt_forest']">
		<spawn maxcount="3" respawndelay="1" time="Day" entitygroup="ZombiesBurntForest" />
